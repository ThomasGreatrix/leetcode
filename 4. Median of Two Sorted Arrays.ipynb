   "source": [
    "def findMedianSortedArrays(nums1, nums2):\n",
    "    for i in nums2:\n",
    "        nums1.append(i)\n",
    "    nums1 = sorted(nums1)\n",
    "    nums_len = len(nums1)\n",
    "    if nums_len%2:\n",
    "        return nums1[int(nums_len/2)]\n",
    "    return (nums1[int(nums_len/2)]+nums1[int(nums_len/2)-1])/2"
   ]
